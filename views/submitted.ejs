<!DOCTYPE html>
<html lang="en">
    <head>
        <meta charset="UTF-8" />
        <meta name="viewport" content="width=device-width, initial-scale=1.0" />
        <link rel="stylesheet" href="./css/style2.css" />

        <title>Document</title>
    </head>
    <body>
        <section class="container">
            <h1>Submitted Forms</h1>
            <%response.forEach((it)=>{%>
            <div class="card">
                <h3>Name: <span><%=it.name%></span></h3>
                <h3>Phone Number: <span><%=it.number%></span></h3>
                <h3>Email: <span><%=it.email%></span></h3>
            </div>
            <%})%>
        </section>
    </body>
</html>
